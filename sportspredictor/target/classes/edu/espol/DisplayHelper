package edu.espol;
public class DisplayHelper {
public static void mostrarEventos(EventoDeportivo[] eventos) {
System.out.println("\nEventos creados:");
for (int i = 0; i < eventos.length; i++) {
System.out.printf(" %d. %s: %s%n",
i + 1,
eventos[i].getDeporte().getNombre(),
eventos[i].obtenerDetallesParaDisplay());
}
}
}
